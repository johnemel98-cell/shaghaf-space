  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:setup": "cd server && node -e \"require('child_process').exec('psql -f database/schema.sql', (err, stdout, stderr) => { if (err) console.error(err); else console.log('Database setup complete'); })\"",
